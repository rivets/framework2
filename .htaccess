RewriteEngine on
Options -Indexes +FollowSymlinks
RewriteBase /testfw2
RewriteRule ^ajax.* ajax.php [L,NC,QSA]
RewriteRule ^(assets)/(.*) $1/$2 [L,NC]
RewriteRule ^.*$ index.php [L,QSA]

